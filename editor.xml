<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bMUST\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="BACKGROUND" value="8d0008" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bQUESTION\b.*">
      <option name="FOREGROUND" value="142cc0" />
      <option name="BACKGROUND" value="d4d20a" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>